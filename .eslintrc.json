{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
      "standard-with-typescript",
      "prettier",
      "plugin:prettier/recommended"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "error"
    }
}
